@model ClientMvc.ModelViews.EventsViewModel

@{
    ViewData["Title"] = "Secret Page";
}
<h1>@ViewData["Title"]</h1>

<ul class="navbar-nav flex-grow-1">
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
    </li>
    <li class="nav-item">
        <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Secret">Privacy</a>
    </li>
    @if (User.Identity.IsAuthenticated)
    {
        <li class="nav-item">
            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Logout">Logout</a>
        </li>
    }
</ul>


<ul class="navbar-nav flex-grow-1">
    @foreach (EventDto ev in Model.AllEvents)
    {
        <li class="nav-item">
            <p> @ev.Id </p>

            <p> @ev.IsAllDayEvent </p>
            <p> @ev.BeginTime </p>
            <p> @ev.EndTime </p>

            <p> @ev.Title </p>
            <p> @ev.Message </p>

            <p> @ev.Lattitude </p>
            <p> @ev.Longtitude </p>
            <p> @ev.LocationName </p>

            <p> @ev.UserId </p>
        </li>
    }
</ul>